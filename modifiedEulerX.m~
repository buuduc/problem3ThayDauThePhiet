clc
clear all
% function returnData = modifiedEulerX()
[a,b,c,d] = feval(@(x) x{:}, num2cell([1.2,0.6,0.8,0.3]));
disp([a,b,c,d])
%% input
dx= @(t,x,y) a*x - b*x*y
dy= @(t,x,y) -c*y+d*x*y
t=10;
x0=0.5;
h=0.2;
tEnd=2;

%% handle
T= [];
X=[x0];
T=[t0:h:tEnd]';
K=[0:1:length(T)-1]';
for k=2:length(T)
    K1(k,1)=h*dy(T(k-1),X(k-1));
    K2(k,1)= h*dy(T(k),X(k-1)+K1(k));
    X(k,1)=X(k-1)+(K1(k)+K2(k))/2;
end
returnData=[K,T,K1,K2,X];
table(K,T,K1,K2,X)