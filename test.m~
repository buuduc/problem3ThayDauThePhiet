clc
close all
clear all
a=modifiedEuler();
result=naturalSpline(a.X,a.Y)
X=a.X;
Y=a.Y
syms x
for i= 1: size(result,1)
    fprintf('X trong khoang [%2.2f,%2.2f] : \n',X(i),X(i+1));
    result(i,1)+result(i,2)*(x-X(i))+result(i,3)*(x-X(i))^2+result(i,4)*(x-X(i))^3
end



