clc
clear all
syms v t
%% input
g=9.8;
m=68.1;
c_d=0.25;
T=[0:1:10];
dv=@(v,t) g-(c_d/m)*v.^2;

%% Tinh toan
f_v =v-int(dv,t) %% Lay nguyen ham cua dv theo dt 
F_v =subs(f_v,t,T)
% V_real=solve(F_v(3),v
for i=1:length(F_v)
    resolve=solve(F_v(i),v)
    if length(resolve)>1
        V_real(i)=resolve(2)
    else
         V_real(i)=resolve
    end
end

eulerData=modifiedEuler();
% eulerData.V_Real =double(solve(subs(f_v-v,{t},{eulerData.T})));
% eulerData.saiSo= abs((eulerData.V_Real-eulerData.V)./eulerData.V_Real)*100
R_KuttaData=RungeKutta();
R_KuttaData.V_Real =double(subs(f_v,{v,t},{R_KuttaData.V,R_KuttaData.T}));
R_KuttaData.saiSo= abs((R_KuttaData.V_Real-R_KuttaData.V)./R_KuttaData.V_Real)*100
% T=eulerData.T
% V=eulerData.V
% double(subs(f_v,{v,t},{V,T}))